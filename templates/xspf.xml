$def with (tracklist)
<?xml version='1.0' encoding='UTF-8'?>
<playlist version='1' xmlns='http://xspf.org/ns/0/'>
	<trackList>
	$for count, info, url in tracklist:
		<track>
			<artist>$info.artist</artist>
			<title>$info.title</title>
			<location>$url</location>
		</track>
	</trackList>
</playlist>
